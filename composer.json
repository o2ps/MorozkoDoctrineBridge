{
	"name": "oops/morozko-doctrine-bridge",
	"description": "Doctrine bridge for cache warmup library for Nette Framework.",
	"keywords": ["nette", "doctrine", "cache", "warmup"],
	"type": "library",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jiří Pudil",
			"email": "me@jiripudil.cz",
			"homepage": "https://jiripudil.cz"
		}
	],
	"support": {
		"email": "me@jiripudil.cz",
		"issues": "https://github.com/o2ps/MorozkoDoctrineBridge/issues"
	},
	"require": {
		"php": ">= 7.1.0",
		"doctrine/orm": "^2.4",
		"nette/di": "^2.4.10",
		"oops/morozko": "dev-master"
	},
	"require-dev": {
		"kdyby/doctrine": "^3.2.0",
		"mockery/mockery": "^1.0.0",
		"nette/bootstrap": "^2.4.0",
		"nette/tester": "^2.0.0"
	},
	"autoload": {
		"psr-4": {
			"Oops\\MorozkoDoctrineBridge\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"OopsTests\\MorozkoDoctrineBridge\\": "tests/MorozkoDoctrineBridge"
		}
	}
}
